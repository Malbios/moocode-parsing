{
  "$schema": "https://json.schemastore.org/package",
  "name": "moocode-parsing",
  "version": "0.1.0",
  "description": "Various parsing utilities for MOOcode developers",
  "keywords": [
    "MOO",
    "LambdaMOO",
    "ToastStunt",
    "MOOcode"
  ],
  "author": "Malbios",
  "license": "MIT",
  "bugs": "https://github.com/Malbios/moocode-parsing/issues",
  "repository": "https://github.com/Malbios/moocode-parsing",
  "scripts": {
    "test": "npm run rebuild:grammar && npm run test:mocha",
    "rebuild:grammar": "npm run clean:grammar && npm run build:grammar",
    "clean:grammar": "rimraf ./src/grammar/generated",
    "clean:out": "rimraf ./dist",
    "build:grammar": "java -jar ./bin/antlr-4.13.1-complete.jar -Dlanguage=TypeScript -listener -visitor -o ./src/grammar/generated -lib ./src/grammar ./src/grammar/moocodeParser.g4 ./src/grammar/moocodeLexer.g4",
    "rebuild": "npm run clean:out && npm run build",
    "build": "npx tsc",
    "test-with-coverage": "npx nyc mocha",
    "test:mocha": "npx mocha",
    "lint": "npm run lint:fix",
    "lint:check": "npx eslint .",
    "lint:fix": "npx eslint --fix ."
  },
  "dependencies": {
    "antlr4": "^4.13.1-patch-1",
    "antlr4-c3": "^3.4.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/antlr4": "^4.11.6",
    "@types/chai": "^4.3.16",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.11",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "chai": "^4.4.1",
    "eslint": "^8.57.0",
    "mocha": "^10.4.0",
    "nyc": "^15.1.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  }
}
